{
    "name": "arhitov/laravel-billing",
    "description": "Billing module for laravel projects",
    "version": "0.1.1",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "billing",
        "cashier",
        "credit",
        "subscriptions",
        "omnipay"
    ],
    "homepage": "https://github.com/arhitov/laravel-billing",
    "authors": [
        {
            "name": "Alexander Arhitov",
            "email": "clgsru@gmail.com",
            "homepage": "https://github.com/arhitov/"
        }
    ],
    "require": {
        "php": "^8.2",
        "arhitov/laravel-helpers": "^0.1",
        "arhitov/laravel-package-helpers": "^0.1.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^10",
        "omnipay/common": "^3",
        "php-http/curl-client": "^2.3",
        "php-http/httplug": "^2.4",
        "watson/validating": "^8.2"
    },
    "require-dev": {
        "arhitov/omnipay-yookassa": "^5.0.1",
        "omnipay/dummy": "*",
        "omnipay/tests": "*",
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "Omnipay/Dummy": "YooKassa driver for Omnipay payment processing library",
        "arhitov/omnipay-yookassa": "YooKassa driver for Omnipay payment processing library"
    },
    "autoload": {
        "psr-4": {
            "Arhitov\\LaravelBilling\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arhitov\\LaravelBilling\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}
